{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build C Program",
            "type": "shell",
            "command": "C:/msys64/ucrt64/bin/gcc.exe",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${workspaceFolder}/bin/Win10/${fileBasenameNoExtension}.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}
